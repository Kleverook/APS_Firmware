	<!--#####################################################################################################-->
	<!--############################################# Modes Definition ######################################-->
	<!--#####################################################################################################-->
	<ModeLogicOperator Name="OR">
		<Mode Name="ERTC_Enabled" UserName="Activate Clock Source" UserNameCH="开启时钟源">
			<ModeLogicOperator Name="XOR">
				<Mode Name="ERTC_Enabled" UserName="Activate Clock Source" UserNameCH="开启时钟源">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_ERTC_Activate"/>
					</SignalLogicalOp>
					<Semaphore>ERTC_ENABLED</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		
		<Mode Name="ERTC_Calendar" UserName="Activate Calendar" UserNameCH="开启日历">
			<Condition Expression="ERTC_ENABLED" Diagnostic="Need to select &quot;Activate Clock Source&quot;" DiagnosticCH="需要 &quot;开启时钟源&quot;"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="ERTC_Calendar" UserName="Activate Calendar" UserNameCH="开启日历">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_ERTC_Calendar"/>
					</SignalLogicalOp>
					<Semaphore>ERTC_CALENDAR</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		
		<Mode Name="Alarm A" UserName="Alarm A" UserNameCH="闹钟 A">
			<Condition Expression="ERTC_CALENDAR" Diagnostic="Need to select &quot;Activate Calendar&quot;" DiagnosticCH="需要 &quot;开启日历&quot;"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Alarm A" UserName="Internal Alarm" UserNameCH="闹钟">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_ERTC_Alarm_A_Intern"/>
					</SignalLogicalOp>
					<Semaphore>AlarmA_Intern</Semaphore>
					<Semaphore>ERTC_ALARM_INTERRUPT</Semaphore>
				</Mode>
				<Mode Name="AlarmA_Eventoutput" UserName="Internal Alarm Event output" UserNameCH="闹钟事件输出">
					<Condition Expression="!ERTC_CALIB_OUT&amp;!ERTC_Wakeup_OUT" 
         	                  Diagnostic="Need &quot;ERTC_OUT&quot; not used by other functions" 
            	                   DiagnosticCH="需要 &quot;ERTC_OUT&quot; 未被其他功能使用"/>
					<SignalLogicalOp Name="AND">
						<Signal Name="OUT"/>
					</SignalLogicalOp>
					<Semaphore>AlarmA_Outputed</Semaphore>
					<Semaphore>ERTC_ALARM_INTERRUPT</Semaphore>
					<Semaphore>ERTC_ALARM_A_OUT</Semaphore>
				</Mode>
			</ModeLogicOperator>			
		</Mode>
		
		<Mode Name="Timestamp" UserName="Timestamp Enable" UserNameCH="时间戳开启">
         <Condition Expression="ERTC_CALENDAR&amp;!ERTC_ALARMA_TAMP1&amp;!ERTC_TAMPER_TAMP1&amp;!ERTC_CALIB_TAMP1&amp;!ERTC_Wakeup_TAMP1" 
         	                 Diagnostic="(1) Need to select &quot;Activate Calendar&quot;&lt;br&gt;(2) Need &quot;ERTC_TAMP1&quot; not used by other functions" 
            	                 DiagnosticCH="(1) 需要 &quot;开启日历&quot;&lt;br&gt;(2) 需要 &quot;ERTC_TAMP1&quot; 未被其他功能使用"/>                 
			<ModeLogicOperator Name="XOR">
				<Mode Name="Timestamp" UserName="Timestamp Enable" UserNameCH="时间戳开启">
					<SignalLogicalOp Name="AND">
						<Signal Name="TAMP1"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>ERTC_TIMESTAMP_INTERRUPT</Semaphore>
			<Semaphore>ERTC_TIMESTAMP_TAMP1</Semaphore>
		</Mode>
		
		<Mode Name="Tamper 1" UserName="Tamper1 Enable" UserNameCH="入侵检测1开启">
         <Condition Expression="ERTC_ENABLED&amp;!ERTC_ALARMA_TAMP1&amp;!ERTC_TIMESTAMP_TAMP1&amp;!ERTC_CALIB_TAMP1&amp;!ERTC_Wakeup_TAMP1" 
         	                  Diagnostic="(1) Need to select &quot;Activate Clock Source&quot;&lt;br&gt;(2) Need &quot;ERTC_TAMP1&quot; not used by other functions" 
            	                   DiagnosticCH="(1) 需要 &quot;开启时钟源&quot;&lt;br&gt;(2) 需要 &quot;ERTC_TAMP1&quot; 未被其他功能使用"/>        
			<ModeLogicOperator Name="XOR">
				<Mode Name="Tamper 1" UserName="Tamper1 Enable" UserNameCH="入侵检测1开启">
					<SignalLogicalOp Name="AND">
						<Signal Name="TAMP1"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>ERTC_TAMPER_INTERRUPT</Semaphore>
			<Semaphore>ERTC_TAMPER_TAMP1</Semaphore>
		</Mode>		

		<Mode Name="Calibration" UserName="Calibration" UserNameCH="校准">
         <Condition Expression="ERTC_ENABLED&amp;!ERTC_ALARM_A_OUT&amp;!ERTC_Wakeup_OUT" 
         	                  Diagnostic="(1) Need to select &quot;Activate Clock Source&quot;&lt;br&gt;(2) Need &quot;ERTC_OUT&quot; not used by other functions" 
            	                   DiagnosticCH="(1) 需要 &quot;开启时钟源&quot;&lt;br&gt;(2) 需要&quot;ERTC_OUT&quot; 未被其他功能使用"/>  
			<ModeLogicOperator Name="XOR">
				<Mode Name="Calibration_512Hz" UserName="Calibration 512Hz" UserNameCH="512Hz校准时钟">
					<SignalLogicalOp Name="AND">
						<Signal Name="OUT"/>
					</SignalLogicalOp>
					<Semaphore>CALIB_512</Semaphore>
					<Semaphore>ERTC_CALIB_OUT</Semaphore>
				</Mode>
				<Mode Name="Calibration_1Hz" UserName="Calibration 1Hz" UserNameCH="1Hz校准时钟">
					<SignalLogicalOp Name="AND">
						<Signal Name="OUT"/>
					</SignalLogicalOp>
					<Semaphore>CALIB_1024</Semaphore>
					<Semaphore>ERTC_CALIB_OUT</Semaphore>
				</Mode>
			</ModeLogicOperator>			
		</Mode>
		
		<Mode Name="Reference clock detection" UserName="Reference clock detection" UserNameCH="参考时钟检测">
         <Condition Expression="ERTC_ENABLED" Diagnostic="Need to select &quot;Activate Clock Source&quot;" DiagnosticCH="需要 &quot;开启时钟源&quot;"/>                     
			<ModeLogicOperator Name="XOR">
				<Mode Name="Reference clock detection" UserName="Reference clock detection" UserNameCH="参考时钟检测">
					<SignalLogicalOp Name="AND">
						<Signal Name="REFIN"/>
					</SignalLogicalOp>
					<Semaphore>ERTC_RECLOCK_With_REFIN</Semaphore>
				</Mode>
			</ModeLogicOperator>
		</Mode>
		
		<Mode Name="Wakeup timer" UserName="Wakeup timer" UserNameCH="周期性唤醒定时器">
			 <Condition Expression="ERTC_ENABLED" Diagnostic="Need to select &quot;Activate Clock Source&quot;" DiagnosticCH="需要 &quot;开启时钟源&quot;"/>    
			<ModeLogicOperator Name="XOR">
				<Mode Name="Wakeup timer" UserName="Wakeup timer" UserNameCH="唤醒定时器">
					<SignalLogicalOp Name="AND">
						<Signal Name="VS_ERTC_Wakeup_Intern"/>
					</SignalLogicalOp>
					<Semaphore>Wakeup_Intern</Semaphore>
				</Mode>
				<Mode Name="Wakeup timer Event" UserName="Internal Wakeup timer Event output" UserNameCH="唤醒事件输出">
					<Condition Expression="!ERTC_ALARM_A_OUT&amp;!ERTC_CALIB_OUT" 
         	                  Diagnostic="Need &quot;ERTC_OUT&quot; not used by other functions" 
            	                   DiagnosticCH="需要 &quot;ERTC_OUT&quot; 未被其他功能使用"/> 
					<SignalLogicalOp Name="AND">
						<Signal Name="OUT"/>
					</SignalLogicalOp>
					<Semaphore>Wakeup_Outputed</Semaphore>
					<Semaphore>ERTC_Wakeup_OUT</Semaphore>
				</Mode>
			</ModeLogicOperator>			
		</Mode>
	</ModeLogicOperator>

	<RefSignal IOMode="System" Name="TAMP1"/>
	<RefSignal IOMode="AlternateFunctionPushPull" Name="REFIN"/>
	<RefSignal IOMode="System" Name="OUT"/>
	<!-- Virtual RefSignals-->
	<RefSignal Name="VS_ERTC_Activate" Virtual="true"/>
	<RefSignal Name="VS_ERTC_Calendar" Virtual="true"/>
	<RefSignal Name="VS_ERTC_Alarm_A_Intern" Virtual="true"/>
	<RefSignal Name="VS_ERTC_Wakeup_Intern" Virtual="true"/>
	<Semaphore>ERTCUsed_ForRCC</Semaphore>
