	<RefParameter Comment="Clock Source Select" CommentCH="时钟源选择" DefaultValue="EXINT_TRIGGER_RISING_EDGE" Group="Wakeup timer" GroupCH="周期性唤醒定时器" Name="ClockSourceSelect" Type="list" Reload = "false">
