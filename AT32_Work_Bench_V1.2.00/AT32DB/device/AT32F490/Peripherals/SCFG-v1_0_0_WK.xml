<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<IP>
    <About>System configuration controller</About>
    <AboutCH>系统配置控制器</AboutCH>
    
    <RefParameter Name="I2SFullDuplex" Comment="I2S full duplex" CommentCH="I2S 全双工" DefaultValue="SCFG_FULL_DUPLEX_I2S_NONE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="SCFG_FULL_DUPLEX_I2S_NONE" />
    </RefParameter>
    <RefParameter Name="I2SFullDuplex" Comment="I2S full duplex" CommentCH="I2S 全双工" DefaultValue="SCFG_FULL_DUPLEX_I2S1_I2S3" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="I2S_FullDuplex_I2S1_I2S3_$IpInstance"/>
        <PossibleValue Comment="I2S1 leader and I2S3 affiliated" CommentCH="I2S1主导方且I2S3附属方" Value="SCFG_FULL_DUPLEX_I2S1_I2S3" />
    </RefParameter>
    <RefParameter Name="I2SFullDuplex" Comment="I2S full duplex" CommentCH="I2S 全双工" DefaultValue="SCFG_FULL_DUPLEX_I2S2_I2S3" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="I2S_FullDuplex_I2S2_I2S3_$IpInstance"/>
        <PossibleValue Comment="I2S2 leader and I2S3 affiliated" CommentCH="I2S2主导方且I2S3附属方" Value="SCFG_FULL_DUPLEX_I2S2_I2S3" />
    </RefParameter>
    <RefParameter Name="I2SFullDuplex" Comment="I2S full duplex" CommentCH="I2S 全双工" DefaultValue="SCFG_FULL_DUPLEX_I2S1_I2S2" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="I2S_FullDuplex_I2S1_I2S2_$IpInstance"/>
        <PossibleValue Comment="I2S1 leader and I2S2 affiliated" CommentCH="I2S1主导方且I2S2附属方" Value="SCFG_FULL_DUPLEX_I2S1_I2S2" />
    </RefParameter>
    
    <RefParameter Name="PVM lock enable" Comment="PVM lock enable" CommentCH="PVM锁使能" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE" />
    </RefParameter>
    <RefParameter Name="PVM lock enable" Comment="PVM lock enable" CommentCH="PVM锁使能" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="$IpInstance_PVMLOCK"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE" />
    </RefParameter>
    
    <RefParameter Name="CPU lock enable" Comment="CPU lock enable" CommentCH="CPU锁使能" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE" />
    </RefParameter>
    <RefParameter Name="CPU lock enable" Comment="CPU lock enable" CommentCH="CPU锁使能" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="$IpInstance_CPULOCK"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE" />
    </RefParameter>
    
    <RefParameter Name="SRAM lock enable" Comment="SRAM odd parity error lock enable" CommentCH="SRAM奇偶校验锁使能" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE" />
    </RefParameter>
    <RefParameter Name="SRAM lock enable" Comment="SRAM odd parity error lock enable" CommentCH="SRAM奇偶校验锁使能" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="$IpInstance_SRAMLOCK"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE" />
    </RefParameter>
    
    <RefParameter Name="PB3 Ultra Strength" Comment="PB3 Ultra high sourcing/sinking strength" CommentCH="PB3 超高电流推动/吸入能力" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE" />
    </RefParameter>
    <RefParameter Name="PB3 Ultra Strength" Comment="PB3 Ultra high sourcing/sinking strength" CommentCH="PB3 超高电流推动/吸入能力" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="$IpInstance_PB3Ultra"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE" />
    </RefParameter>
    
     <RefParameter Name="PB10 Ultra Strength" Comment="PB10 Ultra high sourcing/sinking strength" CommentCH="PB10 超高电流推动/吸入能力" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE" />
    </RefParameter>
    <RefParameter Name="PB10 Ultra Strength" Comment="PB10 Ultra high sourcing/sinking strength" CommentCH="PB10 超高电流推动/吸入能力" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="$IpInstance_PB10Ultra"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE" />
    </RefParameter>
    
    <RefParameter Name="PB9 Ultra Strength" Comment="PB9 Ultra high sourcing/sinking strength" CommentCH="PB9 超高电流推动/吸入能力" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE" />
    </RefParameter>
    <RefParameter Name="PB9 Ultra Strength" Comment="PB9 Ultra high sourcing/sinking strength" CommentCH="PB9 超高电流推动/吸入能力" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="$IpInstance_PB9Ultra"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE" />
    </RefParameter>


