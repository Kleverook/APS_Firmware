    	   <Mode Name="Chip_Select" UserName="Chip Select NE2" UserNameCH="片选 NE2">
			<ModeLogicOperator Name="XOR">
				<Mode Name="Chip_Select" UserName="Chip Select NE2" UserNameCH="片选 NE2">
					<SignalLogicalOp Name="AND">
						<Signal Name="NE2"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>$IpInstance_Chip_Select</Semaphore>
	   </Mode>

        <Mode Name="Memory_device_type" UserName="Memory device type" UserNameCH="存储器类型">
            <Condition Expression="$IpInstance_Chip_Select" Diagnostic="Chip Select disabled" DiagnosticCH="未开启片选"/>
            <ModeLogicOperator Name="XOR">
                <Mode Name="Muxed NOR Flash" UserName="Muxed NOR Flash" UserNameCH="复用 NOR Flash">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="NOE"/>
                        <Signal Name="NWE"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Muxed_NOR_Flash</Semaphore>
                    <Semaphore>$IpInstance_NORFlash_PSRAM</Semaphore>
                </Mode>
                <Mode Name="Muxed PSRAM" UserName="Muxed PSRAM" UserNameCH="复用 PSRAM">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="NOE"/>
                        <Signal Name="NWE"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Muxed_PSRAM</Semaphore>
                    <Semaphore>$IpInstance_NORFlash_PSRAM</Semaphore>
                </Mode>
                <Mode Name="LCD" UserName="LCD Interface" UserNameCH="LCD接口">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="NOE"/>
                        <Signal Name="NWE"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD</Semaphore>
                </Mode>
            </ModeLogicOperator>            
        </Mode>
        
        <Mode Name="Data_Bus_Width" UserName="External memory data bus width" UserNameCH="外部存储器数据总线宽度">
            <Condition Expression="$IpInstance_LCD" Diagnostic="Only available for LCD Interface" DiagnosticCH="仅LCD接口有效"/>
            <Semaphore>$IpInstance_Data_Enable</Semaphore>
            <ModeLogicOperator Name="XOR">
                <Mode Name="8_Bits" UserName="8 bits" UserNameCH="8位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0"/>
                        <Signal Name="D1"/>
                        <Signal Name="D2"/>
                        <Signal Name="D3"/>
                        <Signal Name="D4"/>
                        <Signal Name="D5"/>
                        <Signal Name="D6"/>
                        <Signal Name="D7"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_8_Bits_Data</Semaphore>
                </Mode>
                <Mode Name="16_Bits" UserName="16 bits" UserNameCH="16位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0"/>
                        <Signal Name="D1"/>
                        <Signal Name="D2"/>
                        <Signal Name="D3"/>
                        <Signal Name="D4"/>
                        <Signal Name="D5"/>
                        <Signal Name="D6"/>
                        <Signal Name="D7"/>
                        <Signal Name="D8"/>
                        <Signal Name="D9"/>
                        <Signal Name="D10"/>
                        <Signal Name="D11"/>
                        <Signal Name="D12"/>
                        <Signal Name="D13"/>
                        <Signal Name="D14"/>
                        <Signal Name="D15"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_16_Bits_Data</Semaphore>
                </Mode>
            </ModeLogicOperator>            
        </Mode>
        
        <Mode Name="LCD_RS line" UserName="LCD RS line" UserNameCH="LCD命令/数据线">
            <Condition Expression="$IpInstance_LCD" Diagnostic="Only available for LCD Interface" DiagnosticCH="仅LCD接口有效"/>
            <ModeLogicOperator Name="XOR">
                <Mode Name="A0" UserName="A0" UserNameCH="A0">
                	<Condition Expression="$IpInstance_8_Bits_Data" Diagnostic="Only available for External memory data bus width 8 bits" DiagnosticCH="仅外部存储器数据总线宽度8位宽有效"/>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A0"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_0</Semaphore>
                </Mode>
                <Mode Name="A16" UserName="A16" UserNameCH="A16">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A16"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_16</Semaphore>
                </Mode>
                <Mode Name="A17" UserName="A17" UserNameCH="A17">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A17"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_17</Semaphore>
                </Mode>
                <Mode Name="A18" UserName="A18" UserNameCH="A18">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A18"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_18</Semaphore>
                </Mode>
                <Mode Name="A19" UserName="A19" UserNameCH="A19">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A19"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_19</Semaphore>
                </Mode>
                <Mode Name="A20" UserName="A20" UserNameCH="A20">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A20"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_20</Semaphore>
                </Mode>
                <Mode Name="A21" UserName="A21" UserNameCH="A21">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A21"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_21</Semaphore>
                </Mode>
                <Mode Name="A22" UserName="A22" UserNameCH="A22">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A22"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_22</Semaphore>
                </Mode>
                <Mode Name="A23" UserName="A23" UserNameCH="A23">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="A23"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_LCD_RS_line</Semaphore>
                    <Semaphore>$IpInstance_LCD_RS_line_23</Semaphore>
                </Mode>
            </ModeLogicOperator>            
        </Mode>
        
        <Mode Name="Data_Address_Bus" UserName="Data/Address" UserNameCH="数据/地址线">
            <Condition Expression="$IpInstance_NORFlash_PSRAM" Diagnostic="Memory device type should be Muxed NOR Flash or Muxed PSRAM" DiagnosticCH="存储器类型必须为复用 NOR Flash或者复用 PSRAM"/>
            <Semaphore>$IpInstance_Data_Address_Bus</Semaphore>
            <ModeLogicOperator Name="XOR">
                <Mode Name="8_Bits" UserName="8 bits" UserNameCH="8位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D7"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_8_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="9_Bits" UserName="9 bits" UserNameCH="9位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D8"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_9_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="10_Bits" UserName="10 bits" UserNameCH="10位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D9"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_10_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="11_Bits" UserName="11 bits" UserNameCH="11位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D10"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_11_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="12_Bits" UserName="12 bits" UserNameCH="12位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D11"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_12_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="13_Bits" UserName="13 bits" UserNameCH="13位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D12"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_13_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="14_Bits" UserName="14 bits" UserNameCH="14位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D13"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_14_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="15_Bits" UserName="15 bits" UserNameCH="15位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D14"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_15_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="16_Bits" UserName="16 bits" UserNameCH="16位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_16_Bits_DA</Semaphore>
                </Mode>
                
                <Mode Name="17_Bits" UserName="17 bits" UserNameCH="17位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_17_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="18_Bits" UserName="18 bits" UserNameCH="18位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A17"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_18_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="19_Bits" UserName="19 bits" UserNameCH="19位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A18"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_19_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="20_Bits" UserName="20 bits" UserNameCH="20位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A19"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_20_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="21_Bits" UserName="21 bits" UserNameCH="21位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A20"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_21_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="22_Bits" UserName="22 bits" UserNameCH="22位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A21"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_22_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="23_Bits" UserName="23 bits" UserNameCH="23位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A22"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_23_Bits_DA</Semaphore>
                </Mode>
                <Mode Name="24_Bits" UserName="24 bits" UserNameCH="24位宽">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="D0--D15"/>
                        <Signal Name="A16--A23"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_24_Bits_DA</Semaphore>
                </Mode>
            </ModeLogicOperator>            
        </Mode>
        
        <Mode Name="Clock" UserName="Clock" UserNameCH="时钟">
            <Condition Expression="$IpInstance_NORFlash_PSRAM" Diagnostic="Only for NOR Flash and PSRAM" DiagnosticCH="仅在NOR Flash和PSRAM时有效"/>
            <ModeLogicOperator Name="XOR">
                <Mode Name="Burst_read" UserName="Burst read" UserNameCH="突发读">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CLK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Burst_read</Semaphore>
                    <Semaphore>$IpInstance_Clock</Semaphore>
                </Mode>
                <Mode Name="Burst_write" UserName="Burst write" UserNameCH="突发写">
                	  <Condition Expression="$IpInstance_Muxed_PSRAM" Diagnostic="Only for PSRAM" DiagnosticCH="仅在PSRAM时有效"/>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CLK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Burst_write</Semaphore>
                    <Semaphore>$IpInstance_Clock</Semaphore>
                </Mode>
                <Mode Name="Burst_read_write" UserName="Burst read write" UserNameCH="突发读/写">
                	  <Condition Expression="$IpInstance_Muxed_PSRAM" Diagnostic="Only for PSRAM" DiagnosticCH="仅在PSRAM时有效"/>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CLK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Burst_read_write</Semaphore>
                    <Semaphore>$IpInstance_Clock</Semaphore>
                </Mode>
            </ModeLogicOperator>            
        </Mode>
        
        <Mode Name="Address_valid" UserName="Address valid" UserNameCH="地址有效">
        	     <Condition Expression="$IpInstance_NORFlash_PSRAM" Diagnostic="Only for NOR Flash and PSRAM" DiagnosticCH="仅在NOR Flash和PSRAM时有效"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Address_valid" UserName="Address valid" UserNameCH="地址有效">
					<SignalLogicalOp Name="AND">
						<Signal Name="NADV"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>$IpInstance_Address_valid</Semaphore>
	   </Mode>
	   
	   <Mode Name="Wait_signal" UserName="Wait signal" UserNameCH="等待信号">
            <Condition Expression="$IpInstance_NORFlash_PSRAM" Diagnostic="Only for NOR Flash and PSRAM" DiagnosticCH="仅在NOR Flash和PSRAM时有效"/>
            <ModeLogicOperator Name="XOR">
                <Mode Name="AsynchronousWait" UserName="Asynchronous" UserNameCH="异步">
                	 <Condition Expression="!$IpInstance_Clock | !$IpInstance_Burst_read_write &amp; $IpInstance_Muxed_PSRAM" 
                	 	                  AndFlag = "false" 
                	  	                  Diagnostic="Only for asynchronous accesses"
                	  	                  DiagnosticCH="仅用于异步访问"/>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="NWAIT"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_AsynchronousWait</Semaphore>
                    <Semaphore>$IpInstance_WaitSignal</Semaphore>
                </Mode>
                <Mode Name="Synchronous" UserName="Synchronous" UserNameCH="同步">
                	  <Condition Expression="$IpInstance_Clock" 
                	  	                   Diagnostic="Only for synchronous accesses"
                	  	                   DiagnosticCH="仅用于同步访问"/>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="NWAIT"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Synchronous</Semaphore>
                    <Semaphore>$IpInstance_WaitSignal</Semaphore>
                </Mode>
                <Mode Name="Asynchronous_Synchronous" UserName="Asynchronous Synchronous" UserNameCH="异步/同步">
                	  <Condition Expression="$IpInstance_Burst_read | $IpInstance_Burst_write &amp; $IpInstance_Muxed_PSRAM" 
                	  	                   Diagnostic="Only for PSRAM with either asynchronous read and synchronous write accesses or synchronous read and asynchronous write accesses"
                	  	                   DiagnosticCH="仅用于PSRAM等待信号异步读和同步写或者同步读和异步写"/>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="NWAIT"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_Asynchronous_Synchronous</Semaphore>
                    <Semaphore>$IpInstance_WaitSignal</Semaphore>
                </Mode>
            </ModeLogicOperator>            
        </Mode>
        
        <Mode Name="Byte_select" UserName="Byte select signal" UserNameCH="字节选择信号">
        	     <Condition Expression="$IpInstance_Muxed_PSRAM &amp;  $IpInstance_16_Bits_DA | $IpInstance_17_Bits_DA | $IpInstance_18_Bits_DA | $IpInstance_19_Bits_DA | $IpInstance_20_Bits_DA | $IpInstance_21_Bits_DA | $IpInstance_22_Bits_DA | $IpInstance_23_Bits_DA | $IpInstance_24_Bits_DA" 
        	     	                 Diagnostic="Only for 16-bit PSRAM"
        	     	                 DiagnosticCH="仅用于16位宽PSRAM"/>
			<ModeLogicOperator Name="XOR">
				<Mode Name="Byte_select" UserName="Byte select signal" UserNameCH="字节选择信号">
					<SignalLogicalOp Name="AND">
						<Signal Name="LB"/>
						<Signal Name="UB"/>
					</SignalLogicalOp>
				</Mode>
			</ModeLogicOperator>
			<Semaphore>$IpInstance_Address_valid</Semaphore>
	   </Mode>
        
    </ModeLogicOperator>
	
