    <RefMode Name="I2S full duplex"  Abstract = "true">
    	      <Parameter Name="PVM lock enable" />
    	      <Parameter Name="CPU lock enable" />
    	      <Parameter Name="SRAM lock enable" />
    	      <Parameter Name="PB8 Ultra Strength" />
    	      <Parameter Name="PB9 Ultra Strength" />
    	      <Parameter Name="PA11 PA12 remap" />
    </RefMode>

    <ModeLogicOperator Name="OR"> 
         <Mode Name="PVM lock enable" UserName="PVM lock enable" UserNameCH="PVM锁使能">
            <ModeLogicOperator Name="OR">
                <Mode Name="PVM lock enable" UserName="PVM lock enable" UserNameCH="PVM锁使能">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PVMLOCK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PVMLOCK</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="SRAM odd parity error lock enable" UserName="SRAM odd parity error lock enable" UserNameCH="SRAM奇偶校验锁使能">
            <Annotation Note="Connect or Disconnect TMRx brake input. &lt;br&gt;" 
        	  	                   NoteCH="使能或断开与TMRx 刹车输入的连接。&lt;br&gt;"/>
        	 <ModeLogicOperator Name="OR">
                <Mode Name="SRAM odd parity error lock enable" UserName="SRAM odd parity error lock enable" UserNameCH="SRAM奇偶校验锁使能">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_SRAMLOCK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_SRAMLOCK</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="CPU lock enable" UserName="CPU lock enable" UserNameCH="CPU锁使能">
            <ModeLogicOperator Name="OR">
                <Mode Name="CPU lock enable" UserName="CPU lock enable" UserNameCH="CPU锁使能">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_CPULOCK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_CPULOCK</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>

        <Mode Name="PB8 Ultra Strength" UserName="PB8 Ultra high sourcing/sinking strength" UserNameCH="PB8 超高电流推动/吸入能力">
            <ModeLogicOperator Name="OR">
                <Mode Name="PB8 Ultra Strength" UserName="PB8 Ultra high sourcing/sinking strength" UserNameCH="PB8超高电流推动/吸入能力">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PB8Ultra"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PB8Ultra</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="PB9 Ultra Strength" UserName="PB9 Ultra high sourcing/sinking strength" UserNameCH="PB9 超高电流推动/吸入能力">
            <ModeLogicOperator Name="OR">
                <Mode Name="PB9 Ultra Strength" UserName="PB9 Ultra high sourcing/sinking strength" UserNameCH="PB9 超高电流推动/吸入能力">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PB9Ultra"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PB9Ultra</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="PA11 PA12 remap" UserName="PA11 PA12 remap to PA9 PA10" UserNameCH="PA11 PA12重映射到PA9 PA10">
           <ModeLogicOperator Name="OR">
                <Mode Name="PA11 PA12 remap" UserName="PA11 PA12 remap to PA9 PA10" UserNameCH="PA11 PA12重映射到PA9 PA10">
                    <Semaphore>Semaphore_PA11PA12_Remap$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PA11PA12_REMAP"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
       
    </ModeLogicOperator>
    <RefSignal IOMode="Analog" Name="VS_PVMLOCK" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_CPULOCK" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_SRAMLOCK" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PB8Ultra" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PB9Ultra" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PA11PA12_REMAP" Virtual="true"/>
</IP>
