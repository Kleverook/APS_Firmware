    <RefMode Name="SCFG"  Abstract = "true">
    	      <Parameter Name="TMR1_DMA_RMP" />
    	      <Parameter Name="I2C1_DMA_RMP" />
    	      <Parameter Name="USART2_DMA_RMP" />
    	      <Parameter Name="SPI2_DMA_RMP" />
    	      <Parameter Name="PA11 PA12 remap" />
    </RefMode>

    <ModeLogicOperator Name="OR">     	
         <Mode Name="TMR1_DMA_RMP" UserName="TMR1 DMA request remap" UserNameCH="TMR1 DMA请求重映射">
           <Annotation Note="TMR1 DMA request remap&lt;br&gt;Disable: No remap (DMA requests for TMR1_CH1, TMR1_CH2 and TMR1_CH3 are remapped on DMA channel 2/3/5)&lt;br&gt;Enable:  Remap 1   (DMA requests for TMR1_CH1, TMR1_CH2 and TMR1_CH3 are remapped on DMA channel 6)&lt;br&gt;" 
           	                   NoteCH="TMR1 DMA请求重映射&lt;br&gt;关闭：无重映射（TMR1_CH1, TMR1_CH2 与TMR1_CH3 的DMA请求分别映射在DMA通道2，通道3和通道5上）&lt;br&gt;开启：重映射1 （TMR1_CH1, TMR1_CH2 与TMR1_CH3的DMA请求映射在DMA通道6上）&lt;br&gt;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="TMR1_DMA_RMP" UserName="TMR1 DMA request remap" UserNameCH="TMR1 DMA请求重映射">
                    <Semaphore>Semaphore_TMR1_DMA_RMP_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_TMR1_DMA_RMP"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
         
         <Mode Name="I2C1_DMA_RMP" UserName="I2C1 DMA request remap" UserNameCH="I2C1 DMA请求重映射">
           <Annotation Note="I2C1 DMA request remap&lt;br&gt;Disable: No remap (DMA requests for I2C1_RX and I2C1_TX are remapped on DMA channel 3 and channel 2)&lt;br&gt;Enable:  Remap 1   (DMA requests for I2C1_RX and I2C1_TX are remapped on DMA channel 7 and channel 6)&lt;br&gt;" 
           	                   NoteCH="I2C1 DMA请求重映射&lt;br&gt;关闭：无重映射（I2C1_RX 与I2C1_TX的DMA请求分别映射在DMA通道3和通道2上）&lt;br&gt;开启：重映射1 （I2C1_RX 与I2C1_TX的DMA请求分别映射在DMA通道7和通道6上）&lt;br&gt;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="I2C1_DMA_RMP" UserName="I2C1 DMA request remap" UserNameCH="I2C1 DMA请求重映射">
                    <Semaphore>Semaphore_I2C1_DMA_RMP_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_I2C1_DMA_RMP"/>
                    </SignalLogicalOp>                    
                </Mode>
           </ModeLogicOperator>
         </Mode>
         
         <Mode Name="USART2_DMA_RMP" UserName="USART2 DMA request remap" UserNameCH="USART2 DMA请求重映射">
           <Annotation Note="USART2 DMA request remap&lt;br&gt;Disable: No remap (DMA requests for USART2_RX and USART2_TX are remapped on DMA channel 5 and channel 4)&lt;br&gt;Enable:  Remap 1   (DMA requests for USART2_RX and USART2_TX are remapped on DMA channel 6 and channel 7)&lt;br&gt;" 
           	                   NoteCH="USART2 DMA请求重映射&lt;br&gt;关闭：无重映射（USART2_RX与USART2_TX的DMA请求分别映射在DMA通道5和通道4上）&lt;br&gt;开启：重映射1 （USART2_RX与USART2_TX的DMA请求分别映射在DMA通道6和通道7上）&lt;br&gt;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="USART2_DMA_RMP" UserName="USART2 DMA request remap" UserNameCH="USART2 DMA请求重映射">
                    <Semaphore>Semaphore_USART2_DMA_RMP_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_USART2_DMA_RMP"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
         
         <Mode Name="SPI2_DMA_RMP" UserName="SPI2 DMA request remap" UserNameCH="SPI2 DMA请求重映射">
           <Annotation Note="SPI2 DMA request remap&lt;br&gt;Disable: No remap (DMA requests for SPI2_RX and SPI2_TX are remapped on DMA channel 4 and channel 5)&lt;br&gt;Enable:  Remap 1   (DMA requests for SPI2_RX and SPI2_TX are remapped on DMA channel 6 and channel 7)&lt;br&gt;" 
           	                   NoteCH="SPI2 DMA请求重映射&lt;br&gt;关闭：无重映射（SPI2_RX与SPI2_TX的DMA请求分别映射在DMA通道4和通道5上）&lt;br&gt;开启：重映射1 （SPI2_RX与SPI2_TX的DMA请求分别映射在DMA通道6和通道7上）&lt;br&gt;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="SPI2_DMA_RMP" UserName="SPI2 DMA request remap" UserNameCH="SPI2 DMA请求重映射">
                    <Semaphore>Semaphore_SPI2_DMA_RMP_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_SPI2_DMA_RMP"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>

        <Mode Name="PA11 PA12 remap" UserName="PA11 PA12 remap to PA9 PA10" UserNameCH="PA11 PA12重映射到PA9 PA10">
           <ModeLogicOperator Name="OR">
                <Mode Name="PA11 PA12 remap" UserName="PA11 PA12 remap to PA9 PA10" UserNameCH="PA11 PA12重映射到PA9 PA10">
                    <Semaphore>Semaphore_PA11PA12_Remap$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PA11PA12_REMAP"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
    </ModeLogicOperator>
    
    <RefSignal IOMode="Analog" Name="VS_PA11PA12_REMAP" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_TMR1_DMA_RMP" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_I2C1_DMA_RMP" Virtual="true"/>    
    <RefSignal IOMode="Analog" Name="VS_USART2_DMA_RMP" Virtual="true"/>    
    <RefSignal IOMode="Analog" Name="VS_SPI2_DMA_RMP" Virtual="true"/>               
</IP>
