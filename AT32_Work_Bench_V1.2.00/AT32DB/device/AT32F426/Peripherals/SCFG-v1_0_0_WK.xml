<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<IP>
    <About>System configuration controller</About>
    <AboutCH>系统配置控制器</AboutCH>
    
    <RefParameter Name="PA11 PA12 remap" Comment="PA11 PA12 remap to PA9 PA10" CommentCH="PA11 PA12重映射到PA9 PA10" DefaultValue="FALSE" Type="list" Group="" Visible="false">
        <PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE"/>
    </RefParameter>
    <RefParameter Name="PA11 PA12 remap" Comment="PA11 PA12 remap to PA9 PA10" CommentCH="PA11 PA12重映射到PA9 PA10" DefaultValue="TRUE" Type="list" Group="" Visible="false">
        <Condition Diagnostic="" Expression="Semaphore_PA11PA12_Remap$IpInstance"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE"/>
    </RefParameter>
    
    <RefParameter Name="TMR1_DMA_RMP" Comment="TMR1 DMA request remap" CommentCH="TMR1 DMA请求重映射" DefaultValue="FALSE" Type="list"  Visible="false">
		<PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE"/>
	</RefParameter>
	<RefParameter Name="TMR1_DMA_RMP" Comment="TMR1 DMA request remap" CommentCH="TMR1 DMA请求重映射" DefaultValue="TRUE" Type="list"  Visible="false">
		<Condition Diagnostic="" Expression="Semaphore_TMR1_DMA_RMP_$IpInstance"/>
		<PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE"/>
	</RefParameter>
	
	<RefParameter Name="I2C1_DMA_RMP" Comment="I2C1 DMA request remap" CommentCH="I2C1 DMA请求重映射" DefaultValue="FALSE" Type="list"  Visible="false">
		<PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE"/>
	</RefParameter>
	<RefParameter Name="I2C1_DMA_RMP" Comment="I2C1 DMA request remap" CommentCH="I2C1 DMA请求重映射" DefaultValue="TRUE" Type="list"  Visible="false">
		<Condition Diagnostic="" Expression="Semaphore_I2C1_DMA_RMP_$IpInstance"/>
		<PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE"/>
	</RefParameter>
	
	<RefParameter Name="USART2_DMA_RMP" Comment="USART2 DMA request remap" CommentCH="USART2 DMA请求重映射" DefaultValue="FALSE" Type="list"  Visible="false">
		<PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE"/>
	</RefParameter>
	<RefParameter Name="USART2_DMA_RMP" Comment="USART2 DMA request remap" CommentCH="USART2 DMA请求重映射" DefaultValue="TRUE" Type="list"  Visible="false">
		<Condition Diagnostic="" Expression="Semaphore_USART2_DMA_RMP_$IpInstance"/>
		<PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE"/>
	</RefParameter>
	
	<RefParameter Name="SPI2_DMA_RMP" Comment="SPI2 DMA request remap" CommentCH="SPI2 DMA请求重映射" DefaultValue="FALSE" Type="list"  Visible="false">
		<PossibleValue Comment="Disable" CommentCH="关闭" Value="FALSE"/>
	</RefParameter>
	<RefParameter Name="SPI2_DMA_RMP" Comment="SPI2 DMA request remap" CommentCH="SPI2 DMA请求重映射" DefaultValue="TRUE" Type="list"  Visible="false">
		<Condition Diagnostic="" Expression="Semaphore_SPI2_DMA_RMP_$IpInstance"/>
		<PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE"/>
	</RefParameter>


