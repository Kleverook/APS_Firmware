    <RefMode Name="I2S full duplex"  Abstract = "true">
    	      <Parameter Name="I2SFullDuplex" />
    	      <Parameter Name="PVM lock enable" />
    	      <Parameter Name="CPU lock enable" />
    	      <Parameter Name="PD13 Ultra Strength" />
    	      <Parameter Name="PD12 Ultra Strength" />
    	      <Parameter Name="PB8 Ultra Strength" />
    	      <Parameter Name="PB9 Ultra Strength" />
    </RefMode>

    <ModeLogicOperator Name="OR"> 
         <Mode Name="PVM lock enable" UserName="PVM lock enable" UserNameCH="PVM锁使能">
            <ModeLogicOperator Name="OR">
                <Mode Name="PVM lock enable" UserName="PVM lock enable" UserNameCH="PVM锁使能">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PVMLOCK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PVMLOCK</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="CPU lock enable" UserName="CPU lock enable" UserNameCH="CPU锁使能">
            <ModeLogicOperator Name="OR">
                <Mode Name="CPU lock enable" UserName="CPU lock enable" UserNameCH="CPU锁使能">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_CPULOCK"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_CPULOCK</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="PD13 Ultra Strength" UserName="PD13 Ultra high sourcing/sinking strength" UserNameCH="PD13 超高电流推动/吸入能力">
            <ModeLogicOperator Name="OR">
                <Mode Name="PD13 Ultra Strength" UserName="PD13 Ultra high sourcing/sinking strength" UserNameCH="PD13 超高电流推动/吸入能力">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PD13Ultra"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PD13Ultra</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="PD12 Ultra Strength" UserName="PD12 Ultra high sourcing/sinking strength" UserNameCH="PD12 超高电流推动/吸入能力">
            <ModeLogicOperator Name="OR">
                <Mode Name="PD12 Ultra Strength" UserName="PD12 Ultra high sourcing/sinking strength" UserNameCH="PD12 超高电流推动/吸入能力">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PD12Ultra"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PD12Ultra</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="PB8 Ultra Strength" UserName="PB8 Ultra high sourcing/sinking strength" UserNameCH="PB8 超高电流推动/吸入能力">
            <ModeLogicOperator Name="OR">
                <Mode Name="PB8 Ultra Strength" UserName="PB8 Ultra high sourcing/sinking strength" UserNameCH="PB8超高电流推动/吸入能力">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PB8Ultra"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PB8Ultra</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="PB9 Ultra Strength" UserName="PB9 Ultra high sourcing/sinking strength" UserNameCH="PB9 超高电流推动/吸入能力">
            <ModeLogicOperator Name="OR">
                <Mode Name="PB9 Ultra Strength" UserName="PB9 Ultra high sourcing/sinking strength" UserNameCH="PB9超高电流推动/吸入能力">
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_PB9Ultra"/>
                    </SignalLogicalOp>
                    <Semaphore>$IpInstance_PB9Ultra</Semaphore>
                </Mode>                
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="I2S full duplex" UserName="I2S full duplex" UserNameCH="I2S 全双工">
           <ModeLogicOperator Name="OR">
                <Mode Name="I2S1_L_I2S3_A" UserName="I2S1 leader and I2S3 affiliated" UserNameCH="I2S1主导方且I2S3附属方">
                    <Semaphore>I2S_FullDuplex_I2S1_I2S3_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_I2S13"/>
                    </SignalLogicalOp>
                </Mode>
                <Mode Name="I2S2_L_I2S3_A" UserName="I2S2 leader and I2S3 affiliated" UserNameCH="I2S2主导方且I2S3附属方">
                    <Semaphore>I2S_FullDuplex_I2S2_I2S3_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_I2S23"/>
                    </SignalLogicalOp>
                </Mode>
                <Mode Name="I2S1_L_I2S2_A" UserName="I2S1 leader and I2S2 affiliated" UserNameCH="I2S1主导方且I2S2附属方">
                    <Semaphore>I2S_FullDuplex_I2S1_I2S2_$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_I2S12"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
        
    </ModeLogicOperator>
    
    <RefSignal IOMode="Analog" Name="VS_I2S13" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_I2S23" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_I2S12" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PVMLOCK" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_CPULOCK" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PD13Ultra" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PD12Ultra" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PB8Ultra" Virtual="true"/>
    <RefSignal IOMode="Analog" Name="VS_PB9Ultra" Virtual="true"/>
                       
