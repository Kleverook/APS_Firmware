    <RefParameter Comment="UART Printf" CommentCH="串口打印" DefaultValue="TRUE" Group="Advanced Features" GroupCH="高级功能" Name="UART_Printf" Type="list" Visible="false">
        <Condition Diagnostic="" Expression="Printf_$IpInstance_SYSTEM"/>
        <PossibleValue Comment="Enable" CommentCH="开启" Value="TRUE"/>
    </RefParameter>
      
    <!-- RefMode for Asynchronous UART -->
    <RefMode BaseMode="Mode" HalMode="UART" Name="Asynchronous">
        <Parameter Name="Instance"/>
        <Parameter Name="CLOCK_SOURCE"/>
        <Parameter Name="VirtualMode">
            <PossibleValue>VM_ASYNC</PossibleValue>
        </Parameter>
        <!-- Basic Parameters -->
        <Parameter Name="BaudRate"/>
        <Parameter Name="RealBaudRate"/>
        <Parameter Name="Databitnum"/>
        <Parameter Name="Parityselection"/>
        <Parameter Name="STOPbitnum"/>
        <Parameter Name="HwFlowCtl232"/>
        <!-- Advanced Parameters -->
        <Parameter Name="Mode"/>
        <!-- Advanced Features -->
        <Parameter Name="TXPolarityReverse"/>
        <Parameter Name="RXPolarityReverse"/>
        <Parameter Name="PinsSwapping"/>
        <Parameter Name="DTPolarityReverse"/>
        <Parameter Name="MSBTransmitFirst"/>
        <Parameter Name="UART_Printf"/>
    </RefMode>
    
    <RefMode BaseMode="Mode" HalMode="UART" Name="Asynchronous_ReceiveOnly">
        <Parameter Name="Instance"/>
        <Parameter Name="CLOCK_SOURCE"/>
        <Parameter Name="VirtualMode">
            <PossibleValue>VM_ASYNC_R</PossibleValue>
        </Parameter>
        <!-- Basic Parameters -->
        <Parameter Name="BaudRate"/>
        <Parameter Name="RealBaudRate"/>
        <Parameter Name="Databitnum"/>
        <Parameter Name="Parityselection"/>
        <Parameter Name="STOPbitnum"/>
        <Parameter Name="HwFlowCtl232"/>
        <!-- Advanced Parameters -->
        <Parameter Name="Mode"/>
        <!-- Advanced Features -->
        <Parameter Name="TXPolarityReverse"/>
        <Parameter Name="RXPolarityReverse"/>
