    <RefParameter Name="OCCPolarity_1" Comment="CHC Polarity" CommentCH="通道互补极性" DefaultValue="TMR_OUTPUT_ACTIVE_HIGH" Type="list">
        <Condition Diagnostic="" Expression="Semaphore_Channel1C$IpInstance"/>
            <PossibleValue Value="TMR_OUTPUT_ACTIVE_HIGH" Comment="High" CommentCH="高"/>
            <PossibleValue Value="TMR_OUTPUT_ACTIVE_LOW" Comment="Low" CommentCH="低"/>
    </RefParameter>
    <RefParameter Name="OCCPolarity_1" Comment="CHC Polarity" CommentCH="通道互补极性" DefaultValue="TMR_OUTPUT_ACTIVE_HIGH" Type="list" Visible="false">
        <Condition Diagnostic="" Expression="!Semaphore_Channel1C$IpInstance"/>
            <PossibleValue Value="TMR_OUTPUT_ACTIVE_HIGH" Comment="High" CommentCH="高"/>
    </RefParameter>

