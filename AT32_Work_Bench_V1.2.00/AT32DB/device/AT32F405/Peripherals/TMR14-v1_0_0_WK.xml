            <PossibleValue Value="FALSE" Comment="Disable" CommentCH="关闭"/>
    </RefParameter>
    <RefParameter Name="OCCState_1" Comment="CH State" CommentCH="通道状态" DefaultValue="TRUE" Type="list" Visible="false">
