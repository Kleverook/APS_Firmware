    <!-- ModeLogicOperator for TMR -->
    <ModeLogicOperator Name="OR">
        <Mode Name="Activated" UserName="Activated" UserNameCH="启用">
            <Condition Expression="!TimeBase_$IpInstance_SYSTEM" 
        	  	                   Diagnostic="This TMR already used for &quot;Timebase Source&quot; in SYSTEM" 
        	  	                   DiagnosticCH="此TMR已经被用于SYSTEM中&quot;时基源&quot;"/>
            <ModeLogicOperator Name="OR" UserName="Activated" UserNameCH="启用">
                <Mode Name="Activated">
                    <Semaphore>Semaphore_ClockSourceInternal$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Activated$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_Activated"/>
                    </SignalLogicalOp>
                </Mode>
            </ModeLogicOperator>
        </Mode>
        <Mode Name="Channel1 mode" UserName="Channel1 mode" UserNameCH="通道1模式">
        	  <Condition Expression="Semaphore_Activated$IpInstance"
        	  	                   Diagnostic="Need to select &quot;Activated&quot;" 
        	  	                   DiagnosticCH="需要选择 &quot;启用&quot;"/>
            <ModeLogicOperator Name="XOR">
                <Mode Name="Input_direct_mode" UserName="Input direct mode" UserNameCH="直接输入模式">
                	  <Semaphore>Semaphore_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel_1_DMA_Enable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_input_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_input_Channel1_direct$IpInstance</Semaphore>
                    <Semaphore>Semaphore_CH1pinUsed$IpInstance</Semaphore>
                    <Semaphore>Semaphore_CH1_Input_IO$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CH1" Direction="Input"/>
                    </SignalLogicalOp>
                </Mode>
                <Mode Name="Input_direct_mode_remap" UserName="Input direct mode from remap" UserNameCH="从重映射直接输入模式">
                	  <Semaphore>Semaphore_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel_1_DMA_Enable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_input_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_input_Channel1_direct$IpInstance</Semaphore>
                    <Semaphore>Semaphore_input_Channel1_remap$IpInstance</Semaphore>
                    <Semaphore>Semaphore_CH1pinUsed$IpInstance</Semaphore>
                </Mode>
                <Mode Name="Output_without_pin" UserName="Output without pin" UserNameCH="无引脚输出">
                    <Semaphore>Semaphore_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel_1_DMA_Enable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_OC_No_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_clearable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel1x$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_no_output1"/>
                    </SignalLogicalOp>
                </Mode>
                <Mode Name="Output_CH1" UserName="Output_CH1" UserNameCH="通道1输出">
                    <Semaphore>Semaphore_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel_1_DMA_Enable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_OC_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_clearable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel1x$IpInstance</Semaphore>
                    <Semaphore>Semaphore_CH1pinUsed$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CH1" Direction="Output"/>
                    </SignalLogicalOp>
                </Mode>
                <Mode Name="Output_CH1C" UserName="Output by CH1C" UserNameCH="通道1互补输出">
                    <Semaphore>Semaphore_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel_1_DMA_Enable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_OC_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_clearable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel1C$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CH1C"/>
                    </SignalLogicalOp>
                </Mode>
                <Mode Name="Output_CH1_CH1C" UserName="Output by CH1 CH1C" UserNameCH="通道1和通道1互补输出">
                    <Semaphore>Semaphore_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel_1_DMA_Enable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_OC_output_Channel1$IpInstance</Semaphore>
                    <Semaphore>Semaphore_clearable$IpInstance</Semaphore>
                    <Semaphore>Semaphore_output_X_XC$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel1x$IpInstance</Semaphore>
                    <Semaphore>Semaphore_Channel1C$IpInstance</Semaphore>
                    <Semaphore>Semaphore_CH1pinUsed$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="CH1" Direction="Output"/>
                        <Signal Name="CH1C"/>
                    </SignalLogicalOp>
                </Mode>
            </ModeLogicOperator>
        </Mode>
        
        <Mode Name="Enable BRK pin" UserName="Enable BRK pin" UserNameCH="开启刹车引脚">
            <Condition Expression="Semaphore_OC_output_Channel1$IpInstance"
            	                    Diagnostic="At least one channel must be set in output with pin mode"
            	                    DiagnosticCH="需至少一个通道配置为引脚输出模式"/>
            <ModeLogicOperator Name="OR">
                <Mode Name="Enable BRK pin" UserName="Enable BRK pin" UserNameCH="开启刹车引脚">
                    <Semaphore>Semaphore_Enable_BRK_Pin$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="BRK"/>
                    </SignalLogicalOp>
                </Mode>
            </ModeLogicOperator>
        </Mode>

        <Mode Name="Enable One cycle mode" UserName="Enable One cycle mode" UserNameCH="开启单周期模式">
        	 <Condition Expression="Semaphore_Activated$IpInstance"
        	  	                   Diagnostic="Need to select &quot;Activated&quot;" 
        	  	                   DiagnosticCH="需要选择 &quot;启用&quot;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="Enable One cycle mode" UserName="Enable One cycle mode" UserNameCH="开启单周期模式">
                    <Semaphore>Semaphore_Enable_OCM$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_OCM"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
        
        <Mode Name="Enable channel control bit refresh" UserName="Enable channel control bit refresh" UserNameCH="开启通道控制位刷新">
        	 <Condition Expression="Semaphore_Activated$IpInstance"
        	  	                   Diagnostic="Need to select &quot;Activated&quot;" 
        	  	                   DiagnosticCH="需要选择 &quot;启用&quot;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="Enable channel control bit refresh" UserName="Enable channel control bit refresh" UserNameCH="开启通道控制位刷新">
                    <Semaphore>Semaphore_Enable_CCBR$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_CCBR"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
         
         <Mode Name="Enable channel buffer" UserName="Enable channel buffer control" UserNameCH="开启通道缓存控制">
           <Condition Expression="Semaphore_Activated$IpInstance"
        	  	                   Diagnostic="Need to select &quot;Activated&quot;" 
        	  	                   DiagnosticCH="需要选择 &quot;启用&quot;"/>
        	 <Annotation Note="Channel buffer control.&lt;br&gt;This bit acts on channels that have complementary output.&lt;br&gt;Disable: CxEN, CxCEN and CxOCTRL bits are not buffered.&lt;br&gt;Enable: CxEN, CxCEN and CxOCTRL bits are buffered. &lt;br&gt;" 
        	  	                   NoteCH="通道缓存控制(Channel buffer control)&lt;br&gt; 对具有互补输出的通道:&lt;br&gt;关闭：CxEN，CxCEN和CxOCTRL位无缓存。&lt;br&gt;开启：CxEN，CxCEN和CxOCTRL位有缓存。&lt;br&gt;"/>
           <ModeLogicOperator Name="OR">
                <Mode Name="Enable channel buffer" UserName="Enable channel buffer control" UserNameCH="开启通道缓存控制">
                    <Semaphore>Semaphore_Enable_CBuffer$IpInstance</Semaphore>
                    <SignalLogicalOp Name="AND">
                        <Signal Name="VS_CBUFFER"/>
                    </SignalLogicalOp>
                </Mode>
           </ModeLogicOperator>
         </Mode>
		
    </ModeLogicOperator>

    <!-- RefSignal for TMR -->
    <RefSignal IOMode="MuxOutputPushPull" Name="BRK" Direction="Input"/>
    <RefSignal IOMode="MuxOutputPushPullOpenDrain" Name="CH1"/>
    <RefSignal IOMode="MuxOutputPushPullOpenDrain" Name="CH1C" Direction="Output"/>

