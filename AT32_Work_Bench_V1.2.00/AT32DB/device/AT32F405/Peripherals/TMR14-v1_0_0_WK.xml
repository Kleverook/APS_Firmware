    <RefParameter Name="DeadTime" Comment="Dead-time configuration" CommentCH="死区配置" DefaultValue="0" Max="0" Min="0" Type="integer" Group="Break and Dead-time settings" GroupCH="刹车和死区设置" Visible="false">
