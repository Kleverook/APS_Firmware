	<RefParameter Comment="Non Inverting Input" CommentCH="输入不反相" DefaultValue="CMP_NON_INVERTING_PA5" Group="Pinout Parameters" GroupCH="引脚参数" Name="NonInvertingInput" Type="list" Visible="false">
